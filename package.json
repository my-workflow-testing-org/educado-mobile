    "@testing-library/react-native": "^11.5.0",
    "@types/react": "~18.2.45",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^9.37.0",
    "eslint-config-expo": "~7.1.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
