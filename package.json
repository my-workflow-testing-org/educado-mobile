    "prettier-plugin-tailwindcss": "^0.6.14",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3",
    "typescript-eslint": "^8.46.1"
  },
  "engines": {
    "node": ">=18 <19"
  }
}
